{
  "browser_action": {
    "browser_style": true,
    "default_popup": "cookies.html",
    "default_icon": {
      "19": "icons/default19.png",
      "38": "icons/default38.png"
    }
  },
  "manifest_version": 2,
  "name": "Cookie Pot",
  "version": "1.0",
